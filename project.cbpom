project:
    groupId: 'com.agentdid127'
    artifactId: 'CBScript'
    version: '2.1'

    dependencies:
        - groupId: 'org.yaml'
          artifactId: 'snakeyaml'
          version: '1.21'
        - groupId: 'org.apache.maven.shared'
          artifactId: 'maven-invoker'
          version: '3.1.0'

    build:
        defaultGoal: 'clean install'
        finalName: '${project.artifactId}-${project.version}'
        mainClass: 'com.agentdid127.CBScript.Main'
